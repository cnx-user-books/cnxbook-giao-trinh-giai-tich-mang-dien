  <metadata>
  <md:content-id>m30124</md:content-id><md:title>Nghiên cứu tính ổn định của quá trình quá độ</md:title>
  <md:abstract/>
  <md:uuid>72686843-4629-4445-ba5e-3633ecc0700e</md:uuid>
