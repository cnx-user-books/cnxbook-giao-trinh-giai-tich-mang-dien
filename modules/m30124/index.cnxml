                      </m:mrow>
                      <m:mo stretchy="false">∣</m:mo>
                    </m:mrow>
                  </m:mrow>
                  <m:mi/>
                  <m:mtext>cos</m:mtext>
                  <m:mi/>
                  <m:mo stretchy="false">(</m:mo>
                  <m:mi>δ</m:mi>
                  <m:mrow>
                    <m:msub>
                      <m:mi/>
                      <m:mstyle fontsize="8pt">
                        <m:mrow>
                          <m:mrow>
                            <m:mi>i</m:mi>
                            <m:mo stretchy="false">(</m:mo>
                            <m:mi>t</m:mi>
