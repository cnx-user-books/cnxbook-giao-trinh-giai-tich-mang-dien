                  <m:mi/>
                  <m:mi>f</m:mi>
                  <m:mo stretchy="false">)</m:mo>
                  <m:mtext>.</m:mtext>
                  <m:mi/>
                  <m:mi fontstyle="italic">Δt</m:mi>
                </m:mrow>
              </m:mrow>
            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{k rSub { size 8{1i} } = \( ω` rSub { size 8{i \( t \) } }  - 2π`f \)  "." `Δt} {}</m:annotation>
        </m:semantics>
      </m:math>
    </para>
    <para id="id8167081">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:msub>
                      <m:mi>l</m:mi>
                      <m:mstyle fontsize="8pt">
                        <m:mrow>
                          <m:mn>1i</m:mn>
                        </m:mrow>
                      </m:mstyle>
                    </m:msub>
                    <m:mo stretchy="false">=</m:mo>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>π</m:mi>
                        <m:mi/>
                        <m:mi>f</m:mi>
                      </m:mrow>
                      <m:msub>
