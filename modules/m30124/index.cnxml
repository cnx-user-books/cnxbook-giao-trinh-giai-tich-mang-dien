                    </m:msubsup>
                    <m:mo stretchy="false">=</m:mo>
                    <m:mrow>
                      <m:mo stretchy="false">∣</m:mo>
                      <m:mrow>
                        <m:mi/>
                        <m:mi>E</m:mi>
                        <m:msub>
