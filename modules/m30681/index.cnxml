            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{ ital "Sh"` \( x \) =x+ {  {x rSup { size 8{3} } }  over  {3!} } + {  {x rSup { size 8{5} } }  over  {5!} } + "."  "."  "."  "."  "."  "." + "."  "."  "."  "."  "."  "."  "." } {}</m:annotation>
        </m:semantics>
