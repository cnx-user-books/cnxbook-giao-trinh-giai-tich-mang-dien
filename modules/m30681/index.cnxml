                              </m:mrow>
                            </m:mstyle>
                          </m:msup>
                          <m:mrow>
                            <m:mn>5</m:mn>
                            <m:mi>!</m:mi>
                          </m:mrow>
                        </m:mfrac>
                      </m:mrow>
