            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{ dotsvert } {}</m:annotation>
        </m:semantics>
      </m:math>
    </para>
    <para id="id7032444">Ep = Zpi .Ii </para>
    <para id="id7032462">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mo stretchy="false">⋮</m:mo>
