                              <m:mn>1</m:mn>
                              <m:mo stretchy="false">)</m:mo>
                            </m:mrow>
                          </m:mrow>
                        </m:mstyle>
                      </m:msubsup>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mrow>
                        <m:msub>
                          <m:mi>y</m:mi>
                          <m:mstyle fontsize="8pt">
                            <m:mrow>
                              <m:mn>0</m:mn>
                            </m:mrow>
                          </m:mstyle>
                        </m:msub>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mfenced open="" close="">
                          <m:mfrac>
                            <m:mrow>
                              <m:mfrac>
                                <m:mstyle fontstyle="italic">
                                  <m:mrow>
                                    <m:mtext>dy</m:mtext>
                                  </m:mrow>
                                </m:mstyle>
                                <m:mstyle fontstyle="italic">
                                  <m:mrow>
                                    <m:mtext>dx</m:mtext>
                                  </m:mrow>
                                </m:mstyle>
                              </m:mfrac>
                              <m:mrow>
                                <m:msub>
                                  <m:mo stretchy="false">∣</m:mo>
                                  <m:mstyle fontsize="8pt">
                                    <m:mrow>
                                      <m:mn>0</m:mn>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:msub>
                                <m:mo stretchy="false">+</m:mo>
                                <m:mfrac>
                                  <m:mstyle fontstyle="italic">
                                    <m:mrow>
                                      <m:mtext>dy</m:mtext>
                                    </m:mrow>
                                  </m:mstyle>
                                  <m:mstyle fontstyle="italic">
                                    <m:mrow>
                                      <m:mtext>dx</m:mtext>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:mfrac>
                              </m:mrow>
                              <m:msubsup>
                                <m:mo stretchy="false">∣</m:mo>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mn>1</m:mn>
                                  </m:mrow>
                                </m:mstyle>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mrow>
                                      <m:mo stretchy="false">(</m:mo>
                                      <m:mn>0</m:mn>
                                      <m:mo stretchy="false">)</m:mo>
                                    </m:mrow>
                                  </m:mrow>
                                </m:mstyle>
                              </m:msubsup>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:mfrac>
